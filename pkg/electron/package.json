{
  "name": "electron-portal",
  "version": "0.5.0",
  "private": true,
  "main": "./dist/main.js",
  "homepage": "http://supcon.com",
  "repository": {
    "url": "http://gitlab.rd.supcon.com/VisualField-Next/AppDev.git"
  },
  "scripts": {
    "dev": "cross-env PORT=3080 NODE_ENV=development node server.js",
    "dev:electron": "yarn build:dev && cross-env NODE_ENV=development electron ./dist/main.js",
    "build:dev": "yarn clean:dist && concurrently \"yarn build:dev:main \" \"yarn build:dev:renderer\"",
    "build:dev:main": "cross-env webpack --config ./config/webpack.config.main.js --progress --profile --colors",
    "build:dev:renderer": "cross-env webpack --config ./config/webpack.config.renderer.js --progress --profile --colors",
    "start": "yarn build && cross-env NODE_ENV=production electron ./dist/main.js",
    "ele": "electron ./dist/main.js",
    "build": "yarn clean:dist && concurrently \"yarn build:main \" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.main.prod.js --progress --profile --colors",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.renderer.prod.js --progress --profile --colors",
    "dist:win": "yarn clean:release && electron-builder --win --x64",
    "dist:linux": "yarn clean:release && electron-builder --linux",
    "dist:mac": "yarn clean:release && electron-builder --mac",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "clean:all": "yarn clean:dist && yarn clean:release",
    "release": "yarn build && yarn dist:win",
    "test": "node --trace-warnings ./test/runTests.js",
    "test:e2e": "node --trace-warnings ./test/runTests.js e2e",
    "changelog": "standard-version"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/systemjs": "^0.20.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.1",
    "cross-env": "^5.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "prettier": "^1.12.1",
    "ramda": "^0.26.1",
    "react-hot-loader": "^4.8.0",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "single-spa": "^4.1.1",
    "source-map-loader": "^0.2.3",
    "standard-version": "^4.4.0",
    "systemjs": "^3.0.2",
    "ts-import-plugin": "^1.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "zone.js": "^0.8.29"
  },
  "build": {
    "productName": "portal",
    "appId": "supcon.appdev",
    "files": [
      "dist/",
      "node_modules/",
      "./dist/index.html",
      "./dist/main.js.map",
      "./dist/main.js",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": false
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "release"
    }
  },
  "author": {
    "name": "SUPCON",
    "email": "supcon@supcon.com",
    "url": "http://www.supcon.com/"
  },
  "browserslist": [
    ">= 1%",
    "last 5 versions"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "collectCoverage": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
